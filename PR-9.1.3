#include <stdio.h>

void main() 
{
  int i;
  int sum=0;
  int num;
    printf("no. of sum of consecutive number : ");
    scanf("%d",&num);
    
      for(i=1;num>=i;i++)
      {
          sum=sum+i;
      }  
      printf("\nsum is : %d",sum);
}
