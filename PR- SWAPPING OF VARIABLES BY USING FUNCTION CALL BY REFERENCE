
#include<stdio.h>
void main()
{
    int a=10;

    int b=50;
    printf("\n before swapping a is %d and b is %d ",a,b);
    swap(&a,&b);


}
 int swap(int*prt1,int*prt2)
 {

     int                                                                                                       anish=*prt1;
     *prt1=*prt2;
     *prt2=anish;
     printf("\n after swapping a is %d and b is %d ", *prt1,*prt2);
 }
