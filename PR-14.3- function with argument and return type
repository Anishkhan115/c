#include <stdio.h>

void main() {
    int a,b;
    printf("enter 1st number :");
    scanf("%d",&a);
    printf("enter 2nd number :");
    scanf("%d",&b);
    add(a,b);
    int s=a+b;
    printf("sum of numbers are %d",s);

}
int add(int x,int y)
{

     int sum=x+y;
    return sum;
}

